<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Properties>
    <Property name="pattern">[%d{yyyy-MM-dd HH:mm:ss.SSS}][%-5level] %msg%n</Property>
  </Properties>
  <Appenders>
    <Console name="CONSOLE_APPENDER" target="SYSTEM_ERR">
      <PatternLayout pattern="${pattern}"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="TRACE">
      <!-- <AppenderRef ref="CONSOLE_APPENDER" /> -->
    </Root>
  </Loggers>
</Configuration>
